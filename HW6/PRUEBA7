SELECT LUNGCANCER.L, BRONCHITIS.B, SMOKING.S
    , TUBORLUNGCANCER.E
    , TUBERCULOSIS.T
    , SUM(LUNGCANCER.P*BRONCHITIS.P*SMOKING.P*DYSPNOEA.P*TUBORLUNGCANCER.P*TUBERCULOSIS.P*VISITTOASIA.P*XRAY.P) P
FROM LUNGCANCER, BRONCHITIS, SMOKING, DYSPNOEA, TUBORLUNGCANCER, TUBERCULOSIS, VISITTOASIA,XRAY
WHERE 
  SMOKING.S=LUNGCANCER.S
  AND SMOKING.S=BRONCHITIS.S
  AND DYSPNOEA.E=TUBORLUNGCANCER.E
  AND DYSPNOEA.B=BRONCHITIS.B
  AND LUNGCANCER.L=TUBORLUNGCANCER.L
  AND VISITTOASIA.A=TUBERCULOSIS.A
  AND TUBERCULOSIS.T=TUBORLUNGCANCER.T
  AND XRAY.E=TUBORLUNGCANCER.E  
  AND VISITTOASIA.A='n'
  AND SMOKING.S='n'

  
GROUP BY LUNGCANCER.L, BRONCHITIS.B, TUBORLUNGCANCER.E, TUBERCULOSIS.T

ORDER BY P

