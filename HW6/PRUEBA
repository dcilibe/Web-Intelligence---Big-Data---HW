SELECT *
--BRONCHITIS.B, SUM(BRONCHITIS.P*DYSPNOEA.P*TUBORLUNGCANCER.P)
FROM BRONCHITIS
    , DYSPNOEA
    , TUBORLUNGCANCER
    , LUNGCANCER
    , TUBERCULOSIS
    , VISITTOASIA
    , SMOKING
    , XRAY
WHERE
    DYSPNOEA.B=BRONCHITIS.B
    AND DYSPNOEA.E=TUBORLUNGCANCER.E
    AND LUNGCANCER.L=TUBORLUNGCANCER.L
    AND LUNGCANCER.S=SMOKING.S
    AND TUBORLUNGCANCER.T=TUBERCULOSIS.T
    AND TUBERCULOSIS.A=VISITTOASIA.A
    AND XRAY.E=TUBORLUNGCANCER.E
    AND BRONCHITIS.S=SMOKING.S
    
--GROUP BY BRONCHITIS.B

